{
    "version": 4,
    "vendor": {
        "conan": {}
    },
    "include": [
        "build/build/Debug/generators/CMakePresets.json"
    ],
    "configurePresets": [
        {
            "name": "debug",
            "displayName": "debug config",
            "inherits": "conan-debug",
            "description": "Debug config based on 'conan-debug' configure using 'Unix Makefiles' generator",
            "generator": "Ninja",
            "cacheVariables": {
                "IS_DEV": {
                    "type": "BOOL",
                    "value": "ON"
                },
                "CMAKE_EXPORT_COMPILE_COMMANDS": {
                    "type": "BOOL",
                    "value": "ON"
                }
            }
        }
    ],
    "buildPresets": [
        {
            "name": "debug",
            "configurePreset": "debug"
        }
    ]
}
